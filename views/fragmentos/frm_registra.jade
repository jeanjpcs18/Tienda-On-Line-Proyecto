div(id="content")
        //- if 7 <= 9
        //- a(href="") khajh 
        form(class="login-100-form validate-form" method="post" action="/sign_up/save")
            span(class="login100-form-title p-b-40") Login
            
                    
            div(class="text-center p-t-55 p-b-30")
                span(class="txt1") login with email
            div(class="wrap-input100 validate-input m-b-16")
                input(class="input100" type="text" name="nombre" placeholder="Nombre")
                span(class="focus-input100")
            div(class="wrap-input100 validate-input m-b-16")
                input(class="input100" type="text" name="apellido" placeholder="Apellido")
                span(class="focus-input100")
            div(class="wrap-input100 validate-input m-b-16" data-validate="Ingrese la cedula")
                input(class="input100" type="text" name="cedula" placeholder="Cedula")
                span(class="focus-input100")
            div(class="wrap-input100 validate-input m-b-16" data-validate="Please enter email: ex@abc.xyz")
                input(class="input100" type="email" name="email" placeholder="Email")
                span(class="focus-input100")
            div(class="wrap-input100 validate-input m-b-20" data-validate="Please enter the password")
                span(class="btn-show-pass")
                    i(class="fa fa fa-eye")
                input(class="input100" type="password" name="clave" placeholder="Password")
                span(class="focus-input100")
            div(class="wrap-input100 m-b-16")
                input(class="input100" type="text" name="telefono" placeholder="Telefono")
                span(class="focus-input100")
            div(class="container-login100-form-btn")
                button(class="login100-form-btn" type="submit") Login
            div(class="flex-col-c p-t-224")
                
                a(href="/login" class="txt3 bo1 hov1") Inicia sesion
